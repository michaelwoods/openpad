# Google Gemini Settings
# GEMINI_API_KEY="YOUR_API_KEY_HERE"

# OpenAI Settings (Supports local-compatible APIs like LocalAI/vLLM)
# OPENAI_API_KEY="YOUR_API_KEY_HERE"
# OPENAI_BASE_URL="https://api.openai.com/v1"

# Ollama Settings
# OLLAMA_HOST="http://127.0.0.1:11434"
# OLLAMA_API_KEY="optional_key_if_behind_proxy"

# OpenRouter Settings (https://openrouter.ai)
# OPENROUTER_API_KEY="YOUR_API_KEY_HERE"
# OPENROUTER_REFERER="http://localhost:5173"
# OPENROUTER_TITLE="OpenPAD"

# Custom Provider Settings (for any OpenAI-compatible API)
# CUSTOM_PROVIDER_ENABLED="true"
# CUSTOM_PROVIDER_NAME="My Custom Provider"
# CUSTOM_PROVIDER_BASE_URL="https://api.example.com/v1"
# CUSTOM_PROVIDER_API_KEY="YOUR_API_KEY_HERE"
# CUSTOM_PROVIDER_HEADERS='{"X-Custom-Header": "value"}'
